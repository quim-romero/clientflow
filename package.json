{
  "name": "clientflow",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test dev http://localhost:5173 \"cypress run\"",
    "e2e:open": "start-server-and-test dev http://localhost:5173 \"cypress open\"",
    "lhci": "npm run build && lhci autorun --collect.startServerCommand=\"npm run preview\" --collect.url=http://localhost:4173 --collect.settings.output=json,html --upload.target=filesystem --upload.outputDir=./lhci",
    "e2e:ci": "start-server-and-test preview http://localhost:4173 \"cypress run --config baseUrl=http://localhost:4173\"",
    "lhci:collect": "npm run build && lhci collect --url=http://localhost:4173 --startServerCommand='vite preview' --numberOfRuns=3",
    "lhci:report": "lhci upload --target=filesystem --outputDir=./lhci",
    "test:a11y": "cypress run --spec 'cypress/e2e/a11y.cy.*'",
    "open:a11y": "cypress open --e2e --config specPattern='cypress/e2e/a11y.cy.*'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.0",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/unbounded": "^5.2.6",
    "@lhci/cli": "^0.15.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/cypress": "^10.0.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "cypress": "^14.5.4",
    "cypress-axe": "^1.6.0",
    "eslint": "^9.33.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "start-server-and-test": "^2.0.13",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.0",
    "wait-on": "^8.0.4"
  }
}
